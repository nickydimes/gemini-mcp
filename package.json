{
  "name": "@houtini/gemini-mcp",
  "version": "1.4.1",
  "mcpName": "io.github.houtini-ai/gemini",
  "type": "module",
  "description": "Professional Model Context Protocol server for Google Gemini AI models with enterprise-grade features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gemini-mcp": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "google",
    "ai",
    "llm",
    "chatbot",
    "typescript",
    "nodejs",
    "claude",
    "anthropic",
    "generative-ai",
    "machine-learning",
    "artificial-intelligence"
  ],
  "author": "Houtini",
  "license": "Apache-2.0",
  "homepage": "https://github.com/houtini-ai/gemini-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houtini-ai/gemini-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/houtini-ai/gemini-mcp/issues"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "dotenv": "^16.4.5",
    "winston": "^3.14.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    ".env.example",
    "claude_desktop_config_example.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/houtini-ai"
  }
}
